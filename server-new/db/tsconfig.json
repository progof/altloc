{
	"compilerOptions": {
		"baseUrl": "/db/",
		"noUncheckedIndexedAccess": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"skipLibCheck": true,
		"noEmit": false,
		"allowImportingTsExtensions": false,
		"useUnknownInCatchVariables": true,
		"target": "ESNext",
		"outDir": "../dist/db",
		"declaration": false
	}
}